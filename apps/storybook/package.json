    "expo": "^33.0.0-alpha.web.1",
    "expo-cli": "^2.13.0",
    "prettier": "^1.16.4",
    "react": "16.5.0",
    "react-dom": "16.7.0",
    "react-native": "0.59.5",
    "react-native-web": "^0.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@expo/config": "^1.0.3",
    "@expo/webpack-config": "^0.5.3",
    "@storybook/addon-a11y": "5.1.0-alpha.33",
    "@storybook/addon-backgrounds": "5.1.0-alpha.33",
    "@storybook/addon-centered": "5.1.0-alpha.33",
    "@storybook/addon-events": "5.1.0-alpha.33",
